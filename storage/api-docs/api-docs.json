    "paths": {
        "/api/v1/{token}/{event}/{id}/{value}": {
            "post": {
                "tags": [
                    "Transaction"
                ],
                "summary": "Get All Transaction",
                "description": "Returns Transaction Data",
                "operationId": "postTransactionData",
                "parameters": [
                    {
                        "name": "token",
                        "in": "path",
                        "description": "Your Token",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "event",
                        "in": "path",
                        "description": "The event name you created",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Member Key",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "value",
                        "in": "path",
                        "description": "Value obtained by user, e.g : game score",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "readOnly": "true"
                                        },
                                        "data": {
                                            "type": "object",
                                            "readOnly": "true"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
